public class NonRepeatingElement
{

public static void main(String args[])
{
int a[]= {-1,2,-1,3,0};

    abc: for(int i=0;i<a.length;i++)
    {
        for(int j=i+1;j<a.length;j++)
        {
            if(a[i]!=a[j])
                {
                System.out.println("First non-repeating element is: "+a[i]);
                break abc;
                }
        }
    }

}
}
